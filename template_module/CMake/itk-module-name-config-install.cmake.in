#compute installation prefix relative to this file
get_filename_component (_dir  "${CMAKE_CURRENT_LIST_FILE}" PATH)
get_filename_component(_prefix "${_dir}/../../.." ABSOLUTE)

#import the targets
if(NOT TARGET itk-module-name)
  include("${_prefix}/@itk-module-name-targets-install@")
endif()

set(itk-module-name_INCLUDE_DIRS "${_prefix}/include/itk-module-name-@version@")


